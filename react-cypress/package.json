{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm config set registry https://registry.yarnpkg.com && npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/start.ts",
    "test": "npm config set registry https://registry.yarnpkg.com && npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/test.ts"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "no-undef": "error",
      "import/no-anonymous-default-export": [
        "off",
        {
          "allowArrowFunction": true
        }
      ],
      "prefer-const": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/xpath": "2.0.3",
    "@types/cli-color": "2.0.2",
    "@types/node": "20.2.5",
    "@types/uuid": "9.0.1",
    "@types/wait-on": "5.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "chokidar": "3.5.3",
    "cli-color": "2.0.3",
    "cypress": "12.13.0",
    "eslint": "8.42.0",
    "eslint-plugin-cypress": "2.13.3",
    "execa": "5.1.1",
    "get-port": "5.1.1",
    "linq": "4.0.1",
    "rxjs": "7.8.1",
    "rxjs-exhaustmap-with-trailing": "2.1.1",
    "tree-kill": "1.2.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "uuid": "9.0.0",
    "wait-on": "7.0.1"
  }
}
