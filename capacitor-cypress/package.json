{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm config set registry https://registry.yarnpkg.com && npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/start.ts",
    "test": "npm config set registry https://registry.yarnpkg.com && npx -y -p typescript -p ts-node ts-node --skipProject bin/pre_load_configuration.ts && ts-node --skipProject bin/test.ts"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "no-undef": "error",
      "import/no-anonymous-default-export": [
        "off",
        {
          "allowArrowFunction": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/xpath": "2.0.3",
    "@types/node": "18.11.7",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "cypress": "10.10.0",
    "eslint": "8.26.0",
    "eslint-plugin-cypress": "2.12.1",
    "execa": "5.1.1",
    "get-port": "5.1.1",
    "linq": "4.0.0",
    "tree-kill": "1.2.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "uuid": "9.0.0",
    "wait-on": "6.0.1"
  }
}
