{
    "java.configuration.updateBuildConfiguration": "automatic",
    "files.exclude": {
        ".mvn": true,
        ".settings": true,
        ".classpath": true,
        ".factorypath": true,
        ".gitignore": true,
        "diff": true,
        ".project": true,
        "diff.cmd": true,
        "diff.exe": true,
        "mvn": true,
        "mvn.cmd": true,
        ".vscode": true,
        "target": true,
        "diff.dmg": true,
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "java.compile.nullAnalysis.mode": "disabled",
    "java.debug.settings.onBuildFailureProceed": true,
    "java.debug.settings.stepping.skipClasses": [
        "$JDK",
        "$Libraries",
        "java.lang.ClassLoader",
        "org.springframework.cglib.proxy.*"
    ],
    "java.debug.settings.stepping.skipSynthetics": true,
    "java.debug.settings.stepping.skipConstructors": true,
    "java.debug.settings.stepping.skipStaticInitializers": true,
    "extensions.ignoreRecommendations": true,
    "java.silentNotification": true,
}